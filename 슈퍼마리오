#include <iostream>
#include <vector>
using namespace std;

int main()
{
	ios::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);
	
	int N = 10;
	vector<int> A(N,0);
	cin>>A[0];
	for(int i=1; i<N; i++){
		int temp;
		cin>>temp;
		A[i]=A[i-1]+temp;
	}
	
	for(int i=0; i<N; i++){
		if(A[i]==100){
			cout<<A[i]<<"\n";
			break;
		}
		else if(A[i]>100){
			int a = A[i-1];
			int b = A[i];
			if((100-a)>(b-100)){
				cout<<b<<"\n";
			}
			else if((100-a)<(b-100)){
				cout<<a<<"\n";
			}
			else if((100-a)==(b-100)){
				cout<<b<<"\n";
			}
			break;
		}
	}
}
